{
  "name": "mr creator by zgrgrcn",
  "description": "create mr for gitlab projects",
  "version": "3.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/mrCreatorLogo.png"
    }
  },
  "icons": {
    "128": "/images/mrCreatorLogo.png"
  },
  "permissions": ["storage", "contextMenus"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
